{
    "root": true,
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 8
    },
    "rules": {
        "prettier/prettier": [
            "error"
        ]
    }
}
